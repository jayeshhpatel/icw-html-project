/*************************************************
** LESS MIXINS
*************************************************/
.rotate (@deg) { transform: rotate(@deg); -webkit-transform: rotate(@deg); -moz-transform: rotate(@deg); -ms-transform: rotate(@deg); -o-transform: rotate(@deg); }
.brad (@deg) {-webkit-border-radius: @deg; border-radius: @deg;}
.flexWidth(@width) {-ms-flex: 0 0 @width; flex: 0 0 @width; max-width: @width;}
.opacity (@opacity: 0.5) { -webkit-opacity: @opacity; -moz-opacity: @opacity; opacity: @opacity; }
.gradient(@percent; @firstColor; @secondColor) { background-image: -webkit-linear-gradient(@percent, @firstColor, @secondColor); background-image: -moz-linear-gradient(@percent, @firstColor, @secondColor); background-image: -ms-linear-gradient(@percent, @firstColor, @secondColor); background-image: -o-linear-gradient(@percent, @firstColor, @secondColor); background-image: linear-gradient(@percent, @firstColor, @secondColor); }
.skew(@deg) {-moz-transform: skewX(@deg); -webkit-transform: skewX(@deg); -o-transform: skewX(@deg); -ms-transform: skewX(@deg); transform: skewX(@deg);}
.scale(@amount) {-moz-transform: scale(@amount); -webkit-transform: scale(@amount); -o-transform: scale(@amount); -ms-transform: scale(@amount); transform: scale(@amount);}
.ani(@length, @aniType: all) {-webkit-transition: @aniType @length ease; -moz-transition: @aniType @length ease; -ms-transition: @aniType @length ease; -o-transition: @aniType @length ease; transition: @aniType @length ease;}

